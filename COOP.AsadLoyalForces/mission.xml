	<zone id=0 type="SpawnAIModule">
		<name>hostileZone_0</name>
		<info>Zone of enemy precense</info>
		<synhronizedWith>
			<obj type="GameLogic">dzn_dynai_wp</obj>
			<obj type="GameLogic">dzn_dynai_area</obj>
				<trigger type="None">Area for zone</trigger>
		<synhronizedWith>	
	</zone>
	...
