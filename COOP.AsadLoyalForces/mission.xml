<scripts>
	<defines>
		<var name="dzn_base_fobs" type="ARRAY">List of FOBs</var>
		<var name="dzn_base_outposts" type="ARRAY">List of outposts</var>
		
		<define name="RECON_CAR" type="ARRAY">List of ReconCars classnames</define>
		...
		<define name="TRUCK_AMMO" type="ARRAY">List of Ammo truck classnames</define>
		
		<define name="VEHICLES_EMPTY_KIT" type="ARRAY">List of classnames with "empty" kit</define>
		<define name="VEHICLES_COMBAT_KIT" type="ARRAY">List of classnames with "combat" kit - weapons and ammo</define>
		<define name="VEHICLES_AMMOBOX_KIT" type="ARRAY">List of classnames with "ammobox" kit - weapons and ammo</define>
		<define name="VEHICLES_MEDICBOX_KIT" type="ARRAY">List of classnames with "medicbox" kit -medical gear</define>
		
		<define name="SERVICE_GROUND" type="SCRIPT">Function call to spawn Ground Service</define>
		
		
		<define name="LOCATIONS_BLACKLIST" type="ARRAY">List of locations blacklisted from random picking</define>
		
		<var name="dzn_base_guardmanClass" type="STRING">Class of guardsman</var>
		<var name="dzn_base_guardmanKit" type="STRING">Kit of guardman</var>
		
		
	</defines>
	
	
	<dzn_gear>
		<kit purpose="gear" type="" name=""></kit>
		
		<kit purpose="gear" type="random" name="insergentSquadKit">Kits for insurgents</kit>
		
		<kit purpose="gear_box" type="simple" name="vehicleEmptyKit">"Empty" kit for vehicles</kit>
		<kit purpose="gear_box" type="simple" name="vehicleCombatKit">"Combat" kit for vehicles</kit>
		<kit purpose="gear_box" type="simple" name="vehicleAmmoboxKit">"Ammobox" kit for vehicles</kit>
		<kit purpose="gear_box" type="simple" name="vehicleMedicboxKit">"Medicbox" kit for vehicles</kit>
	</dzn_gear>
	
	
	<dzn_dynai>
		
	</dzn_dynai>
</scripts>
